<%= form_with model: @task do |f| %>
  <p>Make your edit here and hit submit<p>
  <div>
  <%= f.label :name %>
  <%= f.text_field :name%>
  </div>
  <div>
  <%= f.label :description %>
  <%= f.text_area :description%>
  </div>
  <div>
  <%= f.label :complete_by %>
  <%= f.text_field :complete_by%>
  </div>
  <%= f.submit 'Edit this task', class: 'book-button'%>
<%end%>