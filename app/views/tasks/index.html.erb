<h1>TASKS</h1>
<ul>
  <% @tasks.each do |task|  %>
    <li>
      <%= task.name %> <%= task.description %> <%= task.completed_at%>
      <%= button_to "Edit Task", edit_task_path(task[:id]), method: :get %>
      <%= button_to "Delete Task", task_path(task[:id]), method: :delete %>
    </li>
  <% end %>
</ul>